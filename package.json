{
    "name": "websrv",
    "description": "Tiny web server based on koa.",
    "version": "1989.6.32",
    "private": false,
    "homepage": "https://github.com/Leask/websrv",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "debug": "node --inspect --trace-warnings debug.js",
        "test": "node --inspect --trace-warnings test.js",
        "updep": "npx ncu -u && npm install && ( git commit -am 'update dependencies' || true )",
        "gitsync": "git pull && git push",
        "prepublishOnly": "npm version patch && npm run updep && npm run gitsync"
    },
    "author": "Leask Wong <i@leaskh.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Leask/websrv"
    },
    "dependencies": {
        "@google-cloud/storage": "^5.4.0",
        "@koa/cors": "^3.1.0",
        "@koa/multer": "^3.0.0",
        "@koa/router": "^10.0.0",
        "http-status": "^1.4.2",
        "koa": "^2.13.0",
        "koa-bodyparser": "^4.3.0",
        "koa-json": "^2.0.2",
        "koa-logger": "^3.2.1",
        "koa-passport": "^4.1.3",
        "koa-session": "^6.1.0",
        "koa-static": "^5.0.0",
        "koa-useragent": "^4.0.0",
        "multer": "^1.4.2",
        "passport-twitter": "^1.0.4",
        "utilitas": "^1989.6.137"
    }
